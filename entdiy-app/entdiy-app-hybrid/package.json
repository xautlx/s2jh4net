{
  "name": "boe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animate.css": "3.5.2",
    "antd-mobile": "1.0.8",
    "arale-qrcode": "3.0.5",
    "classnames": "2.2.5",
    "cropperjs": "1.0.0-rc.1",
    "crypto-js": "3.1.9-1",
    "fastclick": "1.0.6",
    "mime": "1.3.6",
    "moment": "2.18.1",
    "prop-types": "15.5.8",
    "object-path": "0.11.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-redux-loading-bar": "2.8.1",
    "react-router-dom": "4.1.1",
    "react-transition-group": "1.1.1",
    "redux": "3.6.0",
    "redux-persist": "4.6.0",
    "redux-promise-middleware": "4.2.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-import": "1.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react-app": "2.2.0",
    "babel-runtime": "6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cp-cli": "1.1.0",
    "cp-dir": "1.0.3",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "eslint": "3.16.1",
    "eslint-config-react-app": "0.6.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "postcss-pxtorem": "4.0.0",
    "promise": "7.1.1",
    "react-dev-utils": "0.5.2",
    "redux-logger": "3.0.1",
    "style-loader": "0.13.1",
    "svg-sprite-loader": "0.3.0",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:cordova": "npm run build && cp-dir build cordova/www && cp-cli config/api_host.js cordova/www/api_host.js",
    "test": "node scripts/test.js --env=jsdom",
    "ios": "npm run build:cordova && cd cordova && cordova run ios",
    "android": "npm run build:cordova && cd cordova && cordova run android",
    "ios:build": "npm run build:cordova && cd cordova && cordova build ios",
    "android:build": "npm run build:cordova && cd cordova && cordova build android"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-decorators-legacy",
      [
        "import",
        {
          "style": "css",
          "libraryName": "antd-mobile"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
